        margin-left: 2px;
}

th.views-field-name {
        text-align: center;
}

.view-Deployments .view-header {
		margin-bottom: 15px;
}

.view-Deployments .view-header {
        margin-bottom: 15px;
}

.view-Deployments .view-header a:link, .view-Deployments .view-header a:visited {
        color: #42829E;
}

.view-Deployments .view-header a:hover {
        color: #F58431;
}


/*Individual User Pages*/
.node-type-users {
	p.submitted {
		font-style: italic;
	    margin: 1em 0;
	}
	
	.field-name-field-video {
		text-align: center;
	}
}

//A user page, for an authenticated, registered user -- not anonymous
.page-user {
	#content-inner {
		.profile {
			//below we need the field labels and data look presentable that's all
			.field-label {
				font-weight: bold;
			    margin: 0 0 0.2em;
			}
			.field-item {
				margin: 0 0 1em;
			}
		}
	}
}

//Page used when editing/adding users
.page-node-add-users {
	.form-field-name-field-video {
		a {
			&.button.launcher, &.button.remove {
				font-size: 1.2em;
				font-weight: bold;
				padding: 5px;
			}
		}
	}
	.form-item-field-sector-und select {
		height: 11em;
	}
}

form#webform-client-form-164 {
	p {
		border-bottom: 1px solid #DDDDDD;
		font-size: 120%;
		margin: 30px 0 15px;
		padding-bottom: 4px;
		width: 84%;
	}
	.form-item label.option {
		position: relative;
		top: 1px;
	}
}

//touch ups
.form-checkboxes .form-item, .form-radios .form-item {
	margin: 0.4em 0;
}
