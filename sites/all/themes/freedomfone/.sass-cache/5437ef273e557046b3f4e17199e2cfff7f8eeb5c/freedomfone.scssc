/*----------------------------------
CONTENT PAGES
------------------------------------*/

@mixin content-blue-links-orange-hover {
	a:link, a:visited {
		color: #42829E;
	}
	a:hover {
		color: #F58431;
	}
}

@mixin social-media-links {
	.service-links {
	    margin: 13px 0;
	    text-align: center;
		ul.links li {
		    display: inline;
		    list-style-type: none;
		    padding: 0 1.6em 0 0;
		}
	}
}

@mixin story-comment-node-links {
	background-image: none;
    float: right;
    margin-bottom: 0;
    padding-bottom: 10px;
    padding-left: 10px;
    text-align: right;
}

.node-type-story {
	#content-inner {
		
		p {
			&.submitted a {
				font-weight: normal;
			}
			@include content-blue-links-orange-hover;
			a {
				font-weight: bold;
			}
		}
	}
	@include social-media-links;
	.field-name-taxonomy-vocabulary-1 {
		//The links show up at the bottom of post...this reorders them to align horizontally with node links
		float: left;
	    position: relative;
	    top: -38px;
		.field-label {
			font-size: 0.91666em;
		    text-transform: uppercase;
			padding-right: 1.6em;
			float: left;
		}
		.field-items {
			float: left;
			.field-item {
				display: inline;
			    list-style-type: none;
			    padding: 0 1.6em 0 0;
				a {
					text-transform: uppercase;
					color: #42829E;
					font-size: 0.91666em;
					&:hover{
						color: #F58431;
					}
				}
			}
		}
	}
	.node ul.links {
		/*We increase the padding as a hack*/
		padding-bottom: 2.2em;
	}
	.free-nodelinks ul li.comment-add {
		float: right;
	}
}

