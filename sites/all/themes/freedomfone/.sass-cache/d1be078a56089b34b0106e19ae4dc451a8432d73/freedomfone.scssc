:@template"]0/*clear the page of padding*/
#header, #content, #navigation, .region-sidebar-first, .region-sidebar-second, #footer {
	padding: 0;
}

/*styling of body*/
body {
	font-family: "Lucida Grande", Arial, Verdana, Helvetica, Tahoma, "Bitstream Vera Sans", sans-serif;
	color: #666;
}

blockquote {
    border-left: 3px solid #DDDDDD;
    color: #666666;
    font-size: 13px;
    line-height: 15px;
    margin: 20px;
    padding-left: 10px;
}

a:link {
    color: #737373;
    text-decoration: none;
}
a:visited {
    color: #737373;
    text-decoration: none;
}
a:hover, a:focus {
    color: #F58431;
    text-decoration: underline;
}
a:active {
    color: #737373;
}

#page {
    color: #666666;
    font-size: 0.75em;
    line-height: 1.333em;
	background: url("../images/main_bg.gif");
}

#header {
	background-image: url("../images/header_bg.gif");
	background-repeat: no-repeat;
}

#header-inner {
    background-image: url("../images/main_bg.gif");
    background-repeat: no-repeat;
    position: relative;
}

#ff-top-nav {
    margin-right: 30px;
    position: absolute;
    right: 0;
    top: 78px;
}

#main {
    /*background-image: url("../images/main_bg.gif");*/
}

footer {
    background-image: url("../images/footer_bg.gif");
    background-repeat: no-repeat;
    height: 45px;
    margin-bottom: 30px;
}

/*Custom block styles*/
.home-heading {
    text-align: right;
	margin-right: 48px;
	padding-top: 42px;
	p {
		margin: 0;
	}
}

/*Inside content custom styles*/
.view-interactive-graphic {
	#views_slideshow_pager_field_item_top_interactive_graphic-block_1_0 {
		display: none;
	}
	.views-slideshow-controls-top {
		background-image: url("../images/ig_menu_bg.png");
		background-repeat: no-repeat;
		float: left;
		height: 282px;
		width: 96px;
		z-index: 1000;
	}
	.views_slideshow_main {
		background-color: #6393AC;
		color: #FFFFFF;
		height: 282px;
		margin-left: 96px;
		.views-field-field-slide-description {
			font-size: 0.917em;
		    padding: 9px 12px;
		}
	}
	#widget_pager_top_interactive_graphic-block_1 {
	    background-image: url("../images/horiz_dot_line.png");
	    background-position: center top;
	    background-repeat: repeat-x;
	    font-size: 0.917em;
	    margin-top: 17px;
	    padding-top: 1px;
	    text-transform: uppercase;
	}
	.views_slideshow_pager_field_item {
	    background-image: url("../images/horiz_dot_line.png");
	    background-position: center bottom;
	    background-repeat: repeat-x;
	    height: 33px;
		.views-field-field-slide-title {
		    height: 23px;
		    margin-top: 2px;
		    padding: 7px 0 0 12px;
			&:hover {
				background-image: url("../images/arrow_bg.png");
			    background-repeat: no-repeat;
			    color: #FFFFFF;
			    cursor: pointer;
			}
		}
	}
}

/*Front Page Design*/
.front {
	.node-teaser {
    background-image: url("../images/horiz_dot_line.gif");
    background-position: center top;
    background-repeat: repeat-x;
    margin-top: 20px;
    padding-top: 20px;
	}

 	.views-row-first {
		.node-page .node-inner, .node-story header {
			background-image: url("../images/latest_news.gif");
			background-position: left top;
			background-repeat: no-repeat;
			h2 {
				padding-top: 42px;
			}
		}
	}
	.node-story, .node-page {
		.node-title{
			font-size: 1.5em;
			line-height: 1.2em;
		}
		h2.node-title a:visited, h2.node-title a:link {
			color: #F58431;
		}
		h2.node-title a:hover {
			text-decoration: underline;
		}
		.submitted {
			font-size: 0.916em;
		    margin-top: 1.5em;
			a {
				color:#42829E
			}
		}
		ul.links a {
			color: #F58431;
			&:hover {
				text-decoration: underline;
			}
		}
	}
 	article.node-home {
		background-image: url("../images/horiz_dot_line.png");
	    background-position: center top;
	    background-repeat: repeat-x;
	    font-size: 1.25em;
	    line-height: 1.2em;
	    margin-top: 20px;
	    padding-top: 10px;
	}
}

/*Different types of content have different styles*/
@mixin user-page-links {
	a:link, a:visited {
		color: #42829E;
		font-weight: bold;
	}
	a:hover, a:focus {
		color: #F58431;
	}
}

@mixin news-search-content-h1 {
		font-family: Arial, Helvetica, Verdana, Lucida Grande, Tahoma, "Bitstream Vera Sans", sans-serif;
		font-weight: bold;
		font-size: 1.5em; /* 12 x 1.5 = 18px */
    	line-height: 1.2em;
		color: #616161;
		background-image: url(../images/horiz_dot_line.gif);
		background-position: bottom;
		background-repeat: repeat-x;
		margin-top: 0;
		padding-bottom: 0.35em;
    	margin-bottom: 0.7em;
}

@mixin news-search-comment-h2 {
		font-size: 1.16666em; /* 12 x 1.83333 = 14px */
        line-height: 1.3em;
		color: #4D4D4D;
		margin-top: 0;
        margin-bottom: 0.8em;
}

@mixin news-comment-submitted {
	color: #999999;
	font-size: 0.91666em;
	line-height: 1.2em;
	a {
		&:link, &:visited {
			color: #42829E;
		}
	}
}

@mixin teaser-body {
	background-image: url("../images/horiz_dot_line.gif");
    background-position: center bottom;
    background-repeat: repeat-x;
    margin-bottom: 0.7em;
    padding-bottom: 0.5em;
}

@mixin news-comment-read-more {
	background-image: url("../images/horiz_dot_line.gif");
    background-position: center bottom;
    background-repeat: repeat-x;
    display: block;
    margin-bottom: 0.9em;
    padding-bottom: 0.9em;
}

.node-type-page article, .node-type-users article {
	@include user-page-links;
} 

/*Section News Page*/
.section-news {
	
	/*Latest News Page*/
	.view-header {
		background-image: url("../images/horiz_dot_line.gif");
		background-position: center bottom;
		background-repeat: repeat-x;
		color: #616161;
		font-family: Arial,Helvetica,Verdana,Lucida Grande,Tahoma,"Bitstream Vera Sans",sans-serif;
		font-size: 1.5em;
		font-weight: bold;
		line-height: 1.2em;
		margin-bottom: 0.7em;
		margin-top: 0;
		padding-bottom: 0.35em;
	}
	.view-latest-news {
		.views-row {
			margin-bottom: 14px;
		    padding-bottom: 40px;
		}
		.views-field-title {
			h2 {
				color: #4D4D4D;
				font-size: 1.16666em;
				line-height: 1.3em;
				margin-bottom: 0.8em;
				margin-top: 0;
				&:hover {
					color: #F58431;
					text-decoration: underline;
				}
			}
			a {
				&:link, &:visited {
					color: #4D4D4D;
				}
			}
		}
		.submitted {
			@include news-comment-submitted;
		}
	}
	/*The teaser body*/
	.views-field-body .field-content {
		@include teaser-body;
	}
	
	/*The read more and post comment links*/
	span.views-field-view-node, .views-field-comments-link {
		a {
			&:link, &:visited {
				color: #F58431;
			}
		}
		padding: 0 1.6em 0 0;
	}
	
	/*The Category links*/
	.views-field-taxonomy-vocabulary-1 {
		background-image: url("../images/horiz_dot_line.gif");
	    background-position: center top;
	    background-repeat: repeat-x;
	    margin-top: 0.9em;
	    padding-top: 0.9em;
		span.views-label {
			font-size: 0.91666em;
		    text-transform: uppercase;
			padding-right: 1.6em;
			float: left;
		}
		/*The actual category links*/
		.field-content {
			float: left;
			a {
				&:link, &:visited {
					color: #42829E;
					text-transform: uppercase;
				}
				&:hover {
					color: #F58431
				}
			}
		}
	} //End of latest news page
	
	/*Any other news page*/
	h1#page-title {
		@include news-search-content-h1;
	}
	h2.node-title {
		@include news-search-comment-h2;
	}
	.submitted {
		@include news-comment-submitted;
	}
	article.node-teaser .field-name-body .field-item {
		@include teaser-body;
	}
	.node ul.links {
		@include news-comment-read-more;
		/*We increase the padding as a hack*/
		padding-bottom: 2.2em;
		a {
			&:link, &:visited {
				color: #F58431;
			}
			&:hover {
				color: #F58431;
			}
		}
	}
	article.node-teaser {
		background-image: url("../images/horiz_dot_line.gif");
	    background-position: center bottom;
	    background-repeat: repeat-x;
	    margin-bottom: 14px;
	    padding-bottom: 40px;
	}
}

/*This is a display of all news items on a taxonomy page, not using views*/
.page-news-archives.section-news, .page-taxonomy-term.section-news, .page-news-authors.section-news {
	.free-nodelinks ul.links {
		li {
			a:visited, a:link {
			 color: #F58431 !important;
			}
		}
		padding-bottom: 1em;
	}
	.field-name-taxonomy-vocabulary-1 {
	 .field-label {
		float: left;
	    font-size: 0.91666em;
	    padding-right: 1.6em;
	    text-transform: uppercase;
		font-weight: normal;
	 }
	 .field-items .field-item {
		float: left;
		padding: 0 1.6em 0 0;
		a:link, a:visited {
			color: #42829E;
		}
		a:hover {
			color: #F58431;
		}
	 }
	}
}

/*Node links also need to be well aligned on news archive pages*/


/*----------------------------------
CONTENT PAGES
------------------------------------*/

@mixin content-blue-links-orange-hover {
	a:link, a:visited {
		color: #42829E;
	}
	a:hover {
		color: #F58431;
	}
}

@mixin social-media-links {
	.service-links {
	    margin: 13px 0;
	    text-align: center;
		ul.links li {
		    display: inline;
		    list-style-type: none;
		    padding: 0 1.6em 0 0;
		}
	}
}

@mixin story-comment-node-links {
	background-image: none;
    float: right;
    margin-bottom: 0;
    padding-bottom: 10px;
    padding-left: 10px;
    text-align: right;
}

.node-type-story {
	#content-inner {
		p {
			@include content-blue-links-orange-hover;
			a {
				font-weight: bold;
			}
		}
	}
	@include social-media-links;
	.field-name-taxonomy-vocabulary-1 {
		.field-label {
			font-size: 0.91666em;
		    text-transform: uppercase;
			padding-right: 1.6em;
			float: left;
		}
		.field-items {
			float: left;
			.field-item {
				display: inline;
			    list-style-type: none;
			    padding: 0 1.6em 0 0;
				a {
					text-transform: uppercase;
					color: #42829E;
					&:hover{
						color: #F58431;
					}
				}
			}
		}
	}
	.node ul.links {
		/*We increase the padding as a hack*/
		padding-bottom: 2.2em;
	}
	.free-nodelinks ul li.comment-add {
		float: right;
	}
}

/*Users Page*/

#completed {
        background: #FF776B;
        float: left;
        color: white;
        padding: 2px;
        width: 8em;
        font-weight: bold;
        margin: 0 0 6px 0;
}

#running {
        background: #67cc02;
        float: left;
        color: white;
        padding: 2px;
        width: 8em;
        font-weight: bold;
        margin: 0 4px 6px 0;
}

#suspended {
		background: #fe9902;
        float: left;
        color: white;
        padding: 2px;
        width: 8em;
        font-weight: bold;
        margin: 0 4px 6px 0;
}

.pane-deployments {
	float:left;
	clear: both;
	width: 100%;
	h2 {
		color: #4D4D4D;
	    font-size: 1.66666em;
	    line-height: 1.2em;
	    margin-bottom: 0.6em;
	    margin-top: 0.6em;
	}
}

#mapkey {
	overflow: hidden;
        float: right;
}

#mapkey h2 {
        text-align: right;
        display: none;
}

.view-Deployments {
    float: left;
    margin: 0 1em 1em 0;
    width: 100%;
	.views-exposed-widgets {
		width: 100%;
	}
	.views-exposed-form .views-exposed-widget {
	    float: left;
	    padding: 0.5em 5em 0 0;
		&.views-submit-button {
			float: right;
		    padding: 0.5em 0 0 0;
			/*Theming the actual button*/
			.form-submit {
			    background-color: #F58431;
			    border: medium none;
			    color: #FFFFFF;
			    cursor: pointer;
			    font-weight: bold;
			    margin-top: 1.1em;
			    padding: 3px 11px 3px 13px;
			}
		}
	}
	
	/*Theming the users table*/
	.views-table {
		width: 100%;
	}
	
	.views-field-field-link {
		text-align: center;
	}
	
}

#Running {
        background: url("../../../modules/gmap/markers/small/green.png") no-repeat scroll center 0 transparent;
        padding: 3px;
        margin-left: 2px;
}

#Suspended {
        background: url("../../../modules/gmap/markers/small/orange.png") no-repeat scroll center 0 transparent;
        padding: 3px;
        margin-left: 2px;
}

#Completed {
        background: url("../../../modules/gmap/markers/small/red.png") no-repeat scroll center 0 transparent;
        padding: 3px;
        margin-left: 2px;
}

th.views-field-name {
        text-align: center;
}

.view-Deployments .view-header {
		margin-bottom: 15px;
}

.view-Deployments .view-header {
        margin-bottom: 15px;
}

.view-Deployments .view-header a:link, .view-Deployments .view-header a:visited {
        color: #42829E;
}

.view-Deployments .view-header a:hover {
        color: #F58431;
}


/*Individual User Pages*/
.node-type-users {
	p.submitted {
		font-style: italic;
	    margin: 1em 0;
	}
	
	.field-name-field-video {
		text-align: center;
	}
}
